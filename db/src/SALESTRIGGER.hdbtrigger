TRIGGER "SALESTRIGGER"
AFTER INSERT ON "CATALOGMINI_SALES"
REFERENCING NEW ROW AS MYNEWROW
BEGIN
    INSERT INTO "CATALOGMINI_SALES_LOG"
    VALUES (
        SYSUUID,
        'CREATE',
        'NEW',
        :MYNEWROW.SALES_ID,
        :MYNEWROW.CUSTOMER_ID,
        :MYNEWROW.PRODUCT_ID,
        :MYNEWROW.QUANTITY_SOLD,
        :MYNEWROW.SALE_DATE,
        :MYNEWROW.SALE_AMOUNT
    );
END