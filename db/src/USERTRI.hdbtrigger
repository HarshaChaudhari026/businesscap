TRIGGER "USERTRI"
AFTER INSERT ON "CATALOGMINI_USERS"
REFERENCING NEW ROW AS mynewrow
BEGIN
 INSERT INTO "CATALOGMINI_USER_INFO_LOG"
 VALUES (
    SYSUUID,
    'CREATE',
    'NEW',
    :MYNEWROW.USER_ID,
    :MYNEWROW.USER_NAME,
    :MYNEWROW.USER_EMAILID,
    :MYNEWROW.USER_CONTACTNO,
    :MYNEWROW.USER_PURCHASE
 );
END